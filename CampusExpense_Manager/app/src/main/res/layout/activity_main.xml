<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FDFDFD"
    android:outlineProvider="background"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/ibtBudgetSetting"
        android:layout_width="89dp"
        android:layout_height="82dp"
        android:background="@drawable/rounded_bg"
        android:clipToOutline="true"
        android:contentDescription="BudgetSetting"
        android:elevation="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.211"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.233"
        app:srcCompat="@drawable/baseline_account_balance_wallet_24"
        app:tint="#00BCD4" />

    <TextView
        android:id="@+id/txBudgetSetting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Budget Setting "
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.189"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.332" />

    <ImageButton
        android:id="@+id/IbtRecurringExpenses"
        android:layout_width="89dp"
        android:layout_height="82dp"
        android:background="@drawable/rounded_bg"
        android:clipToOutline="true"
        android:contentDescription="BudgetSetting"
        android:elevation="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.211"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.493"
        app:srcCompat="@drawable/outline_assignment_add_24"
        app:tint="#00BCD4" />

    <TextView
        android:id="@+id/tvRecurringExpenses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Recurring Expenses"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.56" />

    <ImageButton
        android:id="@+id/IbtExpenseReports"
        android:layout_width="89dp"
        android:layout_height="82dp"
        android:background="@drawable/rounded_bg"
        android:clipToOutline="true"
        android:contentDescription="BudgetSetting"
        android:elevation="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.697"
        app:srcCompat="@drawable/outline_bar_chart_24"
        app:tint="#00BCD4" />

    <TextView
        android:id="@+id/tvExpenseReports"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Expense Reports"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.753" />

    <ImageButton
        android:id="@+id/IbtExpenseTracking"
        android:layout_width="89dp"
        android:layout_height="82dp"
        android:background="@drawable/rounded_bg"
        android:clipToOutline="true"
        android:contentDescription="BudgetSetting"
        android:elevation="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.776"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.233"
        app:srcCompat="@drawable/outline_add_card_24"
        app:tint="#00BCD4" />

    <TextView
        android:id="@+id/tvExpenseTracking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Expense Tracking"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.83"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.332" />

    <ImageButton
        android:id="@+id/ibtExpenseOverview"
        android:layout_width="89dp"
        android:layout_height="82dp"
        android:background="@drawable/rounded_bg"
        android:clipToOutline="true"
        android:contentDescription="BudgetSetting"
        android:elevation="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.795"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.493"
        app:srcCompat="@drawable/outline_assignment_24"
        app:tint="#00BCD4" />

    <TextView
        android:id="@+id/tvExpenseOverview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Expense Overview"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.56" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:background="@drawable/button_orange_ripple"
            android:contentDescription="Home"
            android:padding="12dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/outline_home_24" />

        <ImageButton
            android:id="@+id/imageButton10"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:background="@drawable/button_orange_ripple"
            android:contentDescription="Infor"
            android:padding="12dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/outline_account_circle_24" />

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="464dp"
        android:layout_height="168dp"
        android:layout_weight="1"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/screenshot_2025_11_18_033453"
        tools:srcCompat="@drawable/screenshot_2025_11_18_033453" />

    <ImageButton
        android:id="@+id/IbtSetting"
        android:layout_width="66dp"
        android:layout_height="52dp"
        android:background="#00FEF7FF"
        android:contentDescription="Setting"
        android:scaleType="fitCenter"
        android:scrollbarSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.122"
        app:srcCompat="@drawable/outline_dehaze_24"
        app:tint="#3F8964" />

    <ImageButton
        android:id="@+id/IbtNotifications"
        android:layout_width="59dp"
        android:layout_height="58dp"
        android:background="#00FEF7FF"
        android:contentDescription="Notifications"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.963"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.116"
        app:srcCompat="@drawable/outline_circle_notifications_24"
        app:tint="#0A4A49" />


</androidx.constraintlayout.widget.ConstraintLayout>